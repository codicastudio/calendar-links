{
    "name": "codica-studio/calendarlinks",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/calendarlinks",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0",
        "spatie/phpunit-snapshot-assertions": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\CalendarLinks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\CalendarLinks\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=no",
        "test": "vendor/bin/phpunit",
        "test:update-snapshots": "vendor/bin/phpunit -d --update-snapshots"
    },
    "config": {
        "sort-packages": true
    }
}
